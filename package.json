{
  "name": "react-boilerplate",
  "version": "0.1.0",
  "description": "A React boilerplate to handle getting started on most applications.",
  "keywords": [
    "boilerplate",
    "esdoc",
    "eslint",
    "mocha",
    "react",
    "react-router",
    "react-helmet",
    "redux",
    "redux-persist",
    "redux-saga",
    "styled-jsx",
    "webpack"
  ],
  "homepage": "https://github.com/eliasthompson/react-boilerplate",
  "bugs": "https://github.com/eliasthompson/react-boilerplate/issues",
  "license": "MIT",
  "author": {
    "name": "Elias Thompson",
    "email": "email@eliasthompson.com",
    "url": "https://github.com/eliasthompson"
  },
  "main": "./src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eliasthompson/react-boilerplate.git"
  },
  "scripts": {
    "start": "npm run dev",
    "lint": "eslint \"./src/*.js\" \"./src/**/*.js\" \"./test/*.js\" \"./test/**/*.js\"",
    "test": "npm run lint --silent && istanbul cover node_modules/mocha/bin/_mocha -- --opts ./test/mocha.opts",
    "test:watch": "nodemon --ext .js -w ./src/ -w ./test/ --exec \"npm run lint --silent && istanbul cover node_modules/mocha/bin/_mocha -- --opts ./test/mocha.opts || true\"",
    "docs": "esdoc -c esdoc.json",
    "docs:watch": "nodemon --ext .js -w ./src/ --exec \"esdoc -c esdoc.json || true\"",
    "build": "NODE_ENV=production node_modules/.bin/webpack",
    "dev": "node_modules/.bin/webpack-serve",
    "prod": "http-server build"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chalk": "^2.4.1",
    "clean-webpack-plugin": "^0.1.18",
    "connect-history-api-fallback": "^1.5.0",
    "copy-webpack-plugin": "^4.5.2",
    "dotenv": "^6.0.0",
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-jsx-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "favicons-webpack-plugin": "0.0.9",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.0.6",
    "http-server": "^0.11.1",
    "istanbul": "^1.0.0-alpha.2",
    "jsdom": "^11.6.2",
    "jsdom-global": "^3.0.2",
    "koa-connect": "^2.0.1",
    "mocha": "^5.0.0",
    "nock": "^9.6.1",
    "node-sass": "^4.5.3",
    "nodemon": "^1.14.12",
    "sinon": "^6.3.2",
    "sinon-chai": "^3.2.0",
    "styled-jsx-plugin-sass": "^0.2.4",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.1.3",
    "webpack-pwa-manifest": "^3.7.1",
    "webpack-serve": "^1.0.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "axios": "^0.18.0",
    "color": "^3.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "gravatar": "^1.6.0",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.10",
    "prop-types": "^15.6.0",
    "react": "^16.5.0",
    "react-dom": "^16.3.2",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-persist": "^5.10.0",
    "redux-saga": "^0.16.0",
    "styled-jsx": "^2.2.6"
  }
}
